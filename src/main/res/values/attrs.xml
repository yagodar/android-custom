<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MaterialCircularProgressBar">
        <!-- ProgressBar Attributes -->
        <attr name="indeterminate" format="boolean"/>
        <attr name="max" format="integer"/>
        <!--android:maxSide-->
        <!--android:minSide-->
        <!--android:mirrorForRtl-->
        <attr name="progress" format="integer"/>

        <!--materialish-progress Attributes-->
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="circleRadius" format="dimension" />
        <attr name="fillRadius" format="boolean" />
        <attr name="barWidth" format="dimension" />
        <attr name="linear_progress" format="boolean" />
    </declare-styleable>

</resources>